items=[x for x in input().split(',')]
items.sort()
print(','.join(items))

#-----输入多行，然后转大写字母，输入回车，结束---------#
lines=[]
while True:
   s=input()
   if s:
     lines.append(s.upper())
   else:
     break

for sentence in lines:
   print(sentence)



#-----------------------------------#
a=[1,2,3,1,2,4]
#列表转集合
set(a)
#集合转列表
b=list(set(a))
#列表排序
sorted(b)
#格式化列表显示
"" .join(b)

