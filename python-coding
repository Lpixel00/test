#-------------------定义类-----------------#
class className(object):
     def __init__(self):
        self.s=""
     def func_name(self):
        self.s=input()
     def func_name2(self):
        print(self.s.upper())
        
#use class
strObj=className()
strObj.func_name()
strObj.func_name2()

     
