#----------分类统计----------------#

d={"A类":0,"B类":0}

dict={"DIGIT":0,"LETTERS":0}
s=input()
for c in s:
   if c.isdigit():
      dict["DIGIT"]+=1
   elif c.isalpha():
      dict["LETTERS"]+=1
   else:
      pass

dict

#--------------------------#

s="zifuchuan123"
for c in s:
   c.isdigit()
   c.isalpha()
   c.islower()
   c.isupper()


